		outreg2 using "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/MK-Table7a.doc", replace ctitle(lifesatisfaction) title(Overall use <30) dec(3)				
	reg ssocialcomp1 overalluse social1 age2 age3 country2 country3 country4 country5 area2 area3 educ1 educ2 educ4 educ5 educ6 civilstatus1 civilstatus3 civilstatus4 employment1 employment2 employment4 employment5 employment6 gender1 gender2 if age<30
		outreg2 using "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/MK-Table7a.doc", append ctitle(socialcomp1) title(Overall use <30) dec(3)				
	reg strust overalluse social1 age2 age3 country2 country3 country4 country5 area2 area3 educ1 educ2 educ4 educ5 educ6 civilstatus1 civilstatus3 civilstatus4 employment1 employment2 employment4 employment5 employment6 gender1 gender2 if age<30
		outreg2 using "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/MK-Table7a.doc", append ctitle(trust) title(Overall use <30) dec(3)					
	reg ssocial1 overalluse age2 age3 country2 country3 country4 country5 area2 area3 educ1 educ2 educ4 educ5 educ6 civilstatus1 civilstatus3 civilstatus4 employment1 employment2 employment4 employment5 employment6 gender1 gender2 if age<30
		outreg2 using "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/MK-Table7a.doc", append ctitle(social1) title(Overall use <30) dec(3)				
	reg sculturalevents overalluse social2 age2 age3 country2 country3 country4 country5 area2 area3 educ1 educ2 educ4 educ5 educ6 civilstatus1 civilstatus3 civilstatus4 employment1 employment2 employment4 employment5 employment6 gender1 gender2 if age<30

*** NORDIC ***

clear
use "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/Data_All/Data_All_190404f.dta"	

	* tests for linear relationships

	sort overalluse lifesatisfaction
	by overalluse: egen oulifesatisfaction=mean(lifesatisfaction)
	twoway line oulifesatisfaction overalluse

	sort overalluse socialcomp1
	by overalluse: egen ousocialcomp1=mean(socialcomp1)
	twoway line ousocialcomp1 overalluse

	sort overalluse trust
	by overalluse: egen outrust=mean(trust)
	twoway line outrust overalluse

	sort overalluse social1
	by overalluse: egen ousocial1=mean(social1)
	twoway line ousocial1 overalluse

	sort overalluse culturalevents
	by overalluse: egen ouculturalevents=mean(culturalevents)
	twoway line ouculturalevents overalluse

	sort overalluse volunteer
	by overalluse: egen ouvolunteer=mean(volunteer)
	twoway line ouvolunteer overalluse



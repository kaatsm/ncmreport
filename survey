clear
set more off, perm
set matsize 5000, perm
import excel "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/Data_All_190404/CSV/Survey.xlsx", firstrow


/******************************

        NAME VARIABLES 
	   
*******************************/

* id variables
rename A respondent
	label var respondent "respondent"
rename B collector
	label var collector "collector"
rename C start
	label var start "start"
rename D end
	label var end "end"

* unnecessary
drop E
drop F
drop G
drop H
drop I

* affect variables
rename Response language
	label var language "1 language"
rename Happy happy	
	label var happy "2 affect - happy"
rename Anxious anxious
	label var anxious "2 affect - anxious"
rename Lonely lonely
	label var lonely "2 affect - lonely"
rename Connectedtoothers connected
	label var connected "2 affect - connected"
rename Bored bored
	label var bored "2 affect - bored"
rename Interested interested
	label var interested "2 affect - interested"
rename Proud proud
	label var proud "2 affect - proud"
rename Ashamed ashamed
	label var ashamed "2 affect - ashamed"

* evaluative variables
rename S lifesatisfaction
	label var lifesatisfaction "3 life satisfaction"
rename T trust
	label var trust "4 generalized trust"
rename U social1
	label var social1 "5 how often do meet socially"
rename V social2
	label var social2 "6 how many ppl do you talk to"
	
* were you on facebook
rename W onfacebook
	label var onfacebook "7 were you just on facebook?"
label var minutes "8 how many minutes?"
label var hours "8 how many hours?"	
rename Postingstatusupdates statusupdates
	label var statusupdates "9 Posting status updates"
rename Sharinglinks sharinglinks
	label var sharinglinks "9 Sharing Links"
rename Commentingonposts commenting
	label var commenting "9 Commenting on posts"
rename Sendingmessages sendingmessages
	label var sendingmessages "9 Sending messages"
rename Scrollingthroughthenewsfeed newsfeed
	label var newsfeed "9 Scrolling through the news feed"
rename Lookingatfriendspages lookingatfriends
	label var lookingatfriends "9 Looking at friends pages"
rename Readingstatusupdates readingstatus
	label var readingstatus "9 Reading status updates"
rename Searchingforevents searchingevents
	label var searchingevents "9 Searching for events"
	
* facebook numbers
rename OpenEndedResponse profilepic
	label var profilepic "10 how many likes on your profile picture"
rename AI fbfriends
	label var fbfriends "11 how many facebook friends"

* social media usage
rename Facebook facebook
	label var facebook "12 how often - facebook"
rename Instagram instagram
	label var instagram "12 how often - instagram"
rename Twitter twitter
	label var twitter "12 how often - twitter"
rename Snapchat snapchat
	label var snapchat "12 how often - snapchat"
rename LinkedIn linkedin	
	label var linkedin "12 how often - linkedin"
rename YouTube youtube	
	label var youtube "12 how often - youtube"
rename Tinder tinder
	label var tinder "12 how often - tinder"
rename Browsecontent passive
	label var passive "13 browse content"
rename Communicatedirectlywithothers active
	label var active "13 communicated directly with others"

* social activity
rename AS culturalevents
	label var culturalevents "14 How often do you participate in cultural events?"
rename AT volunteer
	label var volunteer "15 In the past four weeks, have you performed any voluntary work?"

* friends social activity
rename AU friendsocial1
	label var friendsocial1 "16 How often do you think your friends meet socially with their friends, relatives or work colleagues?"
rename AV friendlifesatisfaction
	label var friendlifesatisfaction "17 Overall, how satisfied do you think your friends are with their lives nowadays?"

* socio-demographic
rename AW whowith
	label var whowith "18 Who were you with when you began completing this survey?"
rename Otherpleasespecify whowithother
	label var whowithother "18 - Other"
rename AY device
	label var device "19 What device are you using to complete this survey?"
rename AZ deviceother
	label var deviceother "19 - Other"
rename BA age
	label var age "20 What is your age?"
rename BB gender
	label var gender "21 What is your gender?"
rename BC genderother
	label var genderother "21 - Other"
rename BD area
	label var area "22 - Which of the following best describes the area you live in?"
rename BE country
	label var country "23 - country"
rename BF immigrant
	label var immigrant "24 - Were you born in the country you currently live?"
rename BG educ
	label var educ "25 - education"
rename BH employment
	label var employment "26 - employment"
rename BI employmentother
	label var employmentother "26 - Other"
rename BJ civilstatus
	label var civilstatus "27 - civil status"
rename BK civilstatusother
	label var civilstatusother "27 - Other"
	
	
/********************************

       MERGE TRANSLATIONS 
	   
********************************/


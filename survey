/**********************************

     CLEAN AND CREATE VARIABLES
	   
**********************************/
	
* replace with dummy variables	
	replace onfacebook=0 if onfacebook==2
	replace volunteer=0 if volunteer==2
	replace immigrant=0 if immigrant==2

* gender
	tab (gender), gen (gender)
		label var gender1 "non binary"
		label var gender2 "male"
		label var gender3 "female"

* gen affect variable
	replace happy=. if happy==6
	replace anxious=. if anxious==6
	replace lonely=. if lonely==6
	replace connected=. if connected==6
	replace bored=. if bored==6
	replace interested=. if interested==6
	replace proud=. if proud==6
	replace ashamed=. if ashamed==6

	gen posaffect=happy+connected+interested+proud
		label var posaffect "positive affect"
	gen negaffect=anxious+lonely+bored+ashamed
		label var negaffect "negative affect"

* gen social comparison variable
	gen socialcomp=. 
	replace socialcomp=0 if social1<friendsocial1
	replace socialcomp=1 if social1>=friendsocial1
		label var socialcomp "=0 if negative social comparison"
	
* gen alone variable
	gen alone=.
	replace alone=0 if whowith==1
	replace alone=1 if whowith==2 | whowith==3 | whowith==4 | whowith==5
		label var alone "=0 if alone, =1 if with others"
	
* gen fb friends variable
	gen fbfriendsnum = real(regexs(1)) if regexm(fbfriends,"([0-9]+)")
		label var fbfriendsnum "number of fb friends"
	order respondent collector start end language happy anxious lonely connected bored interested proud ashamed lifesatisfaction trust social1 social2 onfacebook minutes statusupdates sharinglinks commenting sendingmessages newsfeed lookingatfriends readingstatus searchingevents profilepic fbfriends fbfriends fbfriendsnum

* gen prof pic variable
	gen profilepicnum = real(regexs(1)) if regexm(profilepic,"([0-9]+)")
		label var profilepicnum "number of profile picture likes"
	order respondent collector start end language happy anxious lonely connected bored interested proud ashamed lifesatisfaction trust social1 social2 onfacebook minutes statusupdates sharinglinks commenting sendingmessages newsfeed lookingatfriends readingstatus searchingevents profilepic profilepicnum fbfriends fbfriends fbfriendsnum
	
* gen educ categories
	tab (educ), gen (educ)
		label var educ1 "less than primary school"
		label var educ2 "primary school"
		label var educ3 "secondary school"
		label var educ4 "bachelor's"
		label var educ5 "masters"
		label var educ6 "doctor's"

* gen employment categories
	drop if employment==7
	replace employment=5 if employment==6
	tab (employment), gen (employment)
		label var employment1 "other"
		label var employment2 "student"
		label var employment3 "employed full time"
		label var employment4 "employed part time"
		label var employment5 "unemployed looking"
		label var employment6 "out of the labor market"
	
* gen civilstatus categories
	replace civilstatus=1 if civilstatus==4
	tab (civilstatus), gen (civilstatus)
		label var civilstatus1 "other"
		label var civilstatus2 "single"
		label var civilstatus3 "in a relationship"
		label var civilstatus4 "married"

* gen area dummies
	tab (area), gen (area)
		label var area1 "urban"
		label var area2 "suburban"
		label var area3 "rural"
	
* gen age categories
	gen age1=1 if age<20
		drop if age<13
		replace age1=0 if age>=20
		label var age1 "less than 20"
	gen age2=1 if age<25
		replace age2=0 if age<20
		replace age2=0 if age>=25
		label var age2 "20-24"
	gen age3=1 if age<30
		replace age3=0 if age>=30
		replace age3=0 if age<25
		label var age3 "25-29"
	gen age4=1 if age>=30
		replace age4=0 if age<30
		label var age4 "30+"
	gen age20=1 if (age2==1 | age3==1)
		replace age20=0 if (age1==1 | age4==1)
		label var age20 "20-29"

* friends categories	
	gen friends1=1 if (social2==0 | social2==1)
	gen friends2=1 if (social2==2 | social2==3)
	gen friends3=1 if (social2==4 | social2==5 | social2==6)
	replace friends1=0 if (friends2==1 | friends3==1)
	replace friends2=0 if (friends1==1 | friends3==1)
	replace friends3=0 if (friends1==1 | friends2==1)
	label var friends1 "0-1 friends"
	label var friends2 "2-3 friends"
	label var friends3 "4+ friends"

* gen passive / active score	
	gen passivescore=newsfeed+lookingatfriends+readingstatus
		label var passivescore "newsfeed+lookingatfriends+readingstatus"
	gen activescore=statusupdates+sharinglinks+commenting+sendingmessages
		label var activescore "statusupdates+sharinglinks+commenting+sendingmessages"

* minutes 
	replace hours=0 if hours==.
	replace minutes=0 if (minutes==. & hours>0)
	replace minutes=(minutes+(hours*60))
	replace minutes=. if minutes==0
	replace onfacebook=1 if minutes!=.
	drop hours	
	
* social comparison
	gen socialcomp1=lifesatisfaction-friendlifesatisfaction
		label var socialcomp1 "happiness social comparison"
	gen socialcomp2=social1-friendsocial1 
		label var socialcomp2 "going out social comparison"

* overall social media use
	gen overalluse=facebook+instagram+twitter+snapchat+linkedin+youtube+tinder
		label var overalluse "overall social media use"
	xtile use=overalluse,n(5)
		tab (use), gen (use)
			label var use1 "0 - 20%"
			label var use2 "21 - 40%"
			label var use3 "41 - 60%"
			label var use4 "61 - 80%"
			label var use5 "81 - 100%"
		drop use
	gen useratio=active/passive
		label var useratio "active use / passive use"
	gen activeuser=1 if useratio>1
		replace activeuser=0 if useratio<1
		label var activeuser "=1 for active user, =0 for passive user"
	gen passiveuser=1 if useratio<0
		replace passiveuser=0 if useratio>1
		replace passiveuser=1 if activeuser==0
		label var passiveuser "=1 for passive user, =0 for active user"
					
* standardized dependent variables
	foreach v of var lifesatisfaction socialcomp1 trust social1 culturalevents volunteer  { 
		su `v', meanonly 
		gen s`v' = (`v' - r(min))/(r(max) - r(min)) 
		}
	label var slifesatisfaction "0-1 scaled lifesatisfaction"
	label var ssocialcomp1 "0-1 scaled socialcomp1" 
	label var strust "0-1 scaled trust" 
	label var ssocial1 "0-1 scaled social1" 
	label var sculturalevents "0-1 scaled culturalevents" 
	label var svolunteer "0-1 scaled volunteer"
	
	foreach v of var happy connected interested proud anxious lonely bored ashamed  { 
		su `v', meanonly 
		gen s`v' = (`v' - r(min))/(r(max) - r(min)) 
		}	
	label var shappy "0-1 scaled happy"
	label var sconnected "0-1 scaled connected" 
	label var sinterested "0-1 scaled interested" 
	label var sproud "0-1 scaled proud" 
	label var sanxious "0-1 scaled anxious" 
	label var slonely "0-1 scaled lonely"
	label var sbored "0-1 scaled bored"
	label var sashamed "0-1 scaled ashamed"

	foreach v of var posaffect negaffect {
		su `v', meanonly 
		gen s`v' = (`v' - r(min))/(r(max) - r(min)) 
		}	
	label var sposaffect "0-1 scaled positive affect"
	label var snegaffect "0-1 scaled negative affect" 
	
	
	order respondent collector start end language happy connected interested proud anxious lonely bored ashamed lifesatisfaction trust social1 social2 onfacebook minutes statusupdates sharinglinks commenting sendingmessages newsfeed lookingatfriends readingstatus searchingevents profilepic profilepicnum fbfriends fbfriendsnum facebook instagram twitter snapchat linkedin youtube tinder passive active culturalevents volunteer friendsocial1 friendlifesatisfaction whowith whowithother device deviceother age gender genderother area country immigrant educ employment employmentother civilstatus civilstatusother posaffect negaffect socialcomp alone educ1 educ2 educ3 educ4 educ5 educ6 employment1 employment2 employment3 employment4 employment5 employment6 civilstatus1 civilstatus2 civilstatus3 civilstatus4 age1 age2 age3 age4 country* friends1 friends2 friends3 passivescore activescore


/********************************

    RESTRICT SAMPLE BY COUNTRY 
	   
 *******************************/
 
 
/**** KEEP NORDIC, USA, UK, CANADA ****/

	keep if country==48 | country==60 | country==77 | country==127 | country==168 | country==32 | country==187 | country==185

	tab (country), gen (country)
	
	label var country1 "canada"
	label var country2 "denmark"
	label var country3 "finland"
	label var country4 "iceland"
	label var country5 "norway"
	label var country6 "sweden"
	label var country7 "united kingdom"
	label var country8 "united states"
	
	drop if age<14

	save "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/Data_All/Data_All_190404_allf.dta", replace
	export excel "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/Data_All/Data_All_190404_allf.xlsx", replace firstrow(varlabels)


/**** KEEP ONLY NORDIC ****/	
	
	keep if country==48 | country==60 | country==77 | country==127 | country==168
	
	drop country1 country7 country8
	
	rename country2 country1
	rename country3 country2
	rename country4 country3
	rename country5 country4
	rename country6 country5 
	
 	save "/Users/micahkaats/Desktop/Happiness Research Institute/Digital Wellbeing/Questionnaire/Data_All/Data_All_190404f.dta", replace
	
